class Node:
    def __init__(self,key):
        self.data=key
        self.left=None
        self.right=None
    def Number_of_nodes(self):
        if(self.left!=None and self.right!=None):
            return((self.left.Number_of_nodes()+self.right.Number_of_nodes())+1)
        elif(self.left!=None and self.right==None):
            return(self.left.Number_of_nodes()+1)
        elif(self.right!=None and self.left==None):
            return(self.right.Number_of_nodes()+1)
        else:
            return 1 

root=Node(1)
root.left=Node(2)
root.right=Node(3)
root.left.left=Node(4)
root.left.right=Node(5)
root.right.left=Node(6)
root.right.left.right=Node(9)
root.right.left.right.left=Node(10)
root.left.left.left=Node(7)
root.left.left.left.right=Node(8) 
root.left.left.left.right.left=Node(21)
print(root.Number_of_nodes())
